<% @games.each do |game| %>
  <%= link_to "#{game.id}.", game_path game, :data => {:id => "#{game.id}"}, {:class => "game-button"} %>
<% end %>

<script type="text/javascript" charset="utf-8">
$(function (){
  $(".game-button").on("click", function(){
    var id = parseInt($(".game-button").attr("data-id"))
  });
});
</script>
